{
  "name": "ngSpark",
  "version": "1.0.0",
  "description": "Boilerplate code for creating an AngularJS web application",
  "main": "build/index.html",
  "author": "Steve Mahony",
  "license": "MIT",
  "repository": "https://github.com/BowlerDo0d/ngSpark.git",
  "browserify": {
    "transform": [
      "babelify",
      "browserify-ngannotate"
    ]
  },
  "nyc": {
    "exclude": [
      "src/app/**/*.spec.js",
      "src/app/templates.js"
    ]
  },
  "devDependencies": {
    "autoprefixer": "6.7.3",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-istanbul": "3.1.2",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.18.0",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "eslint": "3.15.0",
    "eslint-config-airbnb-base": "11.0.1",
    "eslint-config-angular": "0.5.0",
    "eslint-plugin-angular": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jasmine": "2.2.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "0.10.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "2.28.0",
    "jasmine-core": "2.5.2",
    "karma": "1.4.0",
    "karma-babel-preprocessor": "6.0.1",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "node-sass": "4.5.0",
    "postcss-loader": "1.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "5.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-ui-bootstrap": "2.4.0",
    "angular-ui-router": "0.2.15",
    "bootstrap-sass": "3.3.7",
    "font-awesome": "4.5.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && webpack -p",
    "clean": "rimraf dist",
    "serve": "webpack-dev-server",
    "test": "cross-env NODE_ENV=test karma start"
  }
}
